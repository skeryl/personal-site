{
  "name": "personal-site",
  "version": "0.0.0",
  "dependencies": {},
  "devDependencies": {
    "yarn": "^1.22.4",
    "prettier": "^2.0.5",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "yarn workspace personal-site-server run start",
    "format": "prettier --trailing-comma all --write \"./packages/**/*.ts\"",
    "watch:ui": "yarn workspace personal-site-ui run watch",
    "watch:server": "yarn workspace personal-site-server run watch",
    "watch": "run-p -n watch:*"
  },
  "private": true,
  "workspaces": ["packages/*"]
}
